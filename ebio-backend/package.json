{
  "name": "ebio-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "start": "cross-env NODE_ENV=production pm2 start ./bin/www --node-args='--harmony' --name 'ebio-backend'",
    "stop": "cross-env NODE_ENV=production pm2 stop ./bin/www --name 'ebio-backend'",
    "restart": "cross-env NODE_ENV=production pm2 restart ./bin/www --node-args='--harmony' --name 'ebio-backend'"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-winston": "^2.5.1",
    "http-errors": "~1.6.2",
    "mongodb": "^3.0.6",
    "mongoose": "^5.0.14",
    "morgan": "~1.9.0",
    "pm2": "^2.10.2",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "nodemon": "^1.17.3"
  }
}
